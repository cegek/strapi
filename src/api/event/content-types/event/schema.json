{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "shortDescription": {
      "type": "richtext"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "featured": {
      "type": "enumeration",
      "enum": [
        "upcoming",
        "past event"
      ],
      "required": true,
      "default": "past event"
    },
    "ctaButton": {
      "type": "component",
      "repeatable": false,
      "component": "shared.button"
    }
  }
}
